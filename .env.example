# Ingestion Database Configuration
INGESTION_POSTGRES_SERVER=ingestion_postgres
INGESTION_POSTGRES_USER=ingestion_user
INGESTION_POSTGRES_PASSWORD=ingestion_password
INGESTION_POSTGRES_DB=ingestion_database
INGESTION_POSTGRES_PORT=5433

# Alerting Database Configuration
ALERTING_POSTGRES_SERVER=alerting_postgres
ALERTING_POSTGRES_USER=alerting_user
ALERTING_POSTGRES_PASSWORD=alerting_password
ALERTING_POSTGRES_DB=alerting_database
ALERTING_POSTGRES_PORT=5434

# Redis Configuration
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_PASSWORD=password

# RabbitMQ Configuration
RABBITMQ_USER=guest
RABBITMQ_PASS=guest
RABBITMQ_VHOST=/
RABBITMQ_PORT=5672
RABBITMQ_HOST=rabbitmq
RABBITMQ_MANAGEMENT_PORT=15672

# Application Ports
INGESTION_PORT=8000
ALERTING_PORT=8001

# Application Settings
LOG_LEVEL=INFO
SPEED_VIOLATION_THRESHOLD=90
AUTHORIZED_USERS_CACHE_TTL=3600

# Seeding Configuration
ENABLE_SEEDING=true